<?xml version="1.0" encoding="UTF-8" ?>
<container xmlns="http://symfony.com/schema/dic/services"
           xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
           xsi:schemaLocation="http://symfony.com/schema/dic/services
        http://symfony.com/schema/dic/services/services-1.0.xsd">

    <services>
        <service id="inowas.modflow.boundarymanager" class="Inowas\ModflowBundle\Service\BoundaryManager">
            <argument type="service" id="doctrine.orm.modflow_entity_manager"/>
        </service>
        <service id="inowas.modflow.calculationmanager" class="Inowas\ModflowBundle\Service\CalculationManager">
            <argument type="service" id="doctrine.orm.modflow_entity_manager"/>
        </service>
        <service id="inowas.modflow.headsmanager" class="Inowas\ModflowBundle\Service\HeadsManager">
            <argument type="service" id="doctrine.orm.modflow_entity_manager"/>
        </service>
        <service id="inowas.modflow.modelmanager" class="Inowas\ModflowBundle\Service\ModflowModelManager">
            <argument type="service" id="doctrine.orm.modflow_entity_manager"/>
        </service>
        <service id="inowas.modflow.observationpointmanager" class="Inowas\ModflowBundle\Service\ObservationPointManager">
            <argument type="service" id="doctrine.orm.modflow_entity_manager"/>
        </service>
        <service id="inowas.flopy.packagemanager" class="Inowas\ModflowBundle\Service\FlopyPackageManager"/>
    </services>
</container>
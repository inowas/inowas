<?xml version="1.0" encoding="UTF-8" ?>
<container xmlns="http://symfony.com/schema/dic/services"
           xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
           xsi:schemaLocation="http://symfony.com/schema/dic/services
        http://symfony.com/schema/dic/services/services-1.0.xsd">

    <services>
        <service id="inowas.modflow.boundarymanager" class="Inowas\ModflowBundle\Service\BoundaryManager">
            <argument type="service" id="doctrine.orm.default_entity_manager"/>
        </service>
        <service id="inowas.modflow.calculationmanager" class="Inowas\ModflowBundle\Service\CalculationManager">
            <argument type="service" id="doctrine.orm.default_entity_manager"/>
            <argument type="service" id="kernel"/>
        </service>
        <service id="inowas.modflow.headsmanager" class="Inowas\ModflowBundle\Service\HeadsManager">
            <argument type="service" id="doctrine.orm.default_entity_manager"/>
        </service>
        <service id="inowas.modflow.toolmanager" class="Inowas\ModflowBundle\Service\ModflowToolManager">
            <argument type="service" id="doctrine.orm.default_entity_manager"/>
        </service>
        <service id="inowas.modflow.observationpointmanager" class="Inowas\ModflowBundle\Service\ObservationPointManager">
            <argument type="service" id="doctrine.orm.default_entity_manager"/>
        </service>
        <service id="inowas.flopy.packagemanager" class="Inowas\ModflowBundle\Service\FlopyPackageManager"/>
        <service id="inowas.modflow.file_persister" class="Inowas\ModflowBundle\Service\FilePersister">
            <argument type="service" id="kernel"/>
        </service>
        <service id="inowas.modflow.model.manager" class="Inowas\Modflow\Model\Service\ModflowModelManager">
            <argument type="service" id="inowas.model_boundaries_finder"/>
            <argument type="service" id="inowas.model_scenarios_finder"/>
        </service>
        <service id="inowas.modflow.stressperiod_data_generator" class="Inowas\Modflow\Model\Service\StressPeriodDataGenerator">
            <argument type="service" id="inowas.geotools"/>
        </service>
        <service id="inowas.geotools" class="Inowas\GeoTools\Model\GeoTools">
            <argument type="service" id="doctrine.orm.default_entity_manager"/>
        </service>
        <service id="inowas.soilmodel.manager" class="Inowas\Modflow\Model\Service\SoilmodelManager">
            <argument type="service" id="inowas.soilmodel.layer_values_finder"/>
        </service>
        <service id="inowas.add_modflow_model_boundary_handler" class="Inowas\Modflow\Model\Handler\AddBoundaryHandler">
            <argument type="service" id="modflow_model_list" />
        </service>
        <service id="inowas.add_calculated_head_handler" class="Inowas\Modflow\Model\Handler\AddCalculatedHeadHandler">
            <argument type="service" id="modflow_calculation_list" />
            <argument type="service" id="inowas.modflow.file_persister" />
        </service>
        <service id="inowas.add_calculated_budget_handler" class="Inowas\Modflow\Model\Handler\AddCalculatedBudgetHandler">
            <argument type="service" id="modflow_calculation_list" />
        </service>
        <service id="inowas.add_modflow_scenario_handler" class="Inowas\Modflow\Model\Handler\AddModflowScenarioHandler">
            <argument type="service" id="modflow_model_list" />
        </service>
        <service id="inowas.calculate_active_cells_handler" class="Inowas\Modflow\Model\Handler\CalculateActiveCellsHandler">
            <argument type="service" id="modflow_model_list" />
            <argument type="service" id="inowas.geotools" />
        </service>
        <service id="inowas.change_modflow_model_bounding_box_handler" class="Inowas\Modflow\Model\Handler\ChangeModflowModelBoundingBoxHandler">
            <argument type="service" id="modflow_model_list" />
        </service>
        <service id="inowas.change_modflow_model_description_handler" class="Inowas\Modflow\Model\Handler\ChangeModflowModelDescriptionHandler">
            <argument type="service" id="modflow_model_list" />
        </service>
        <service id="inowas.change_modflow_model_grid_size_handler" class="Inowas\Modflow\Model\Handler\ChangeModflowModelGridSizeHandler">
            <argument type="service" id="modflow_model_list" />
        </service>
        <service id="inowas.calculate_modflow_calculation_handler" class="Inowas\Modflow\Model\Handler\CalculateModflowModelCalculationHandler">
            <argument type="service" id="inowas.modflow_projection.calculation_configuration_finder" />
            <argument type="service" id="inowas.soilmodel.flopy_calculation_service" />
        </service>
        <service id="inowas.change_modflow_model_name_handler" class="Inowas\Modflow\Model\Handler\ChangeModflowModelNameHandler">
            <argument type="service" id="modflow_model_list" />
        </service>
        <service id="inowas.change_modflow_model_soilmodel_id_handler" class="Inowas\Modflow\Model\Handler\ChangeModflowModelSoilmodelIdHandler">
            <argument type="service" id="modflow_model_list" />
        </service>
        <service id="inowas.create_modflow_model_handler" class="Inowas\Modflow\Model\Handler\CreateModflowModelHandler">
            <argument type="service" id="modflow_model_list" />
        </service>

        <service id="inowas.create_modflow_calculation_handler" class="Inowas\Modflow\Model\Handler\CreateModflowModelCalculationHandler">
            <argument type="service" id="modflow_model_list" />
            <argument type="service" id="soil_model_list" />
            <argument type="service" id="modflow_calculation_list" />
            <argument type="service" id="inowas.modflow.model.manager" />
        </service>
        <service id="inowas.remove_modflow_model_boundary_handler" class="Inowas\Modflow\Model\Handler\RemoveBoundaryHandler">
            <argument type="service" id="modflow_model_list" />
        </service>
        <service id="inowas.remove_modflow_scenario_handler" class="Inowas\Modflow\Model\Handler\RemoveModflowScenarioHandler">
            <argument type="service" id="modflow_model_list" />
        </service>
        <service id="inowas.update_modflow_model_boundary_handler" class="Inowas\Modflow\Model\Handler\UpdateBoundaryHandler">
            <argument type="service" id="modflow_model_list" />
        </service>
        <service id="inowas.update_modflow_model_boundary_geometry_handler" class="Inowas\Modflow\Model\Handler\UpdateBoundaryGeometryHandler">
            <argument type="service" id="modflow_model_list" />
        </service>
        <service id="inowas.update_calculation_results_handler" class="Inowas\Modflow\Model\Handler\UpdateCalculationResultsHandler">
            <argument type="service" id="modflow_calculation_list" />
        </service>
        <service id="inowas.update_calculation_package_parameter_handler" class="Inowas\Modflow\Model\Handler\UpdateCalculationPackageParameterHandler">
            <argument type="service" id="modflow_calculation_list" />
        </service>
        <service id="inowas.update_calculation_stress_periods_handler" class="Inowas\Modflow\Model\Handler\UpdateCalculationStressperiodsHandler">
            <argument type="service" id="modflow_calculation_list" />
        </service>
    </services>
</container>

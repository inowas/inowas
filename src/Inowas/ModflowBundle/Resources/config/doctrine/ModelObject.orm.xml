<?xml version="1.0" encoding="UTF-8" ?>
<doctrine-mapping
        xmlns="http://doctrine-project.org/schemas/orm/doctrine-mapping"
        xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
        xsi:schemaLocation="http://doctrine-project.org/schemas/orm/doctrine-mapping
        http://doctrine-project.org/schemas/orm/doctrine-mapping.xsd">

    <mapped-superclass name="Inowas\ModflowBundle\Model\ModelObject">
        <id name="id" type="uuid" column="id"/>
        <field name="name" column="name" type="string" length="255" nullable="true" />
        <field name="activeCells" column="active_cells" type="active_cells" nullable="true" />
        <field name="dateCreated" type="datetime" column="date_created" nullable="true"/>
        <field name="dateModified" type="datetime" column="date_modified" nullable="true"/>
    </mapped-superclass>

</doctrine-mapping>
